#include <stdio.h>

int main(){
    
char letra1;
int numeroint;
float numerofloat;
double double01;
    
    letra1 = 'C';
    numeroint = 10;
    numerofloat = 3.14f;
    double01 = 17.928351;
        
        
        printf("%c\n", letra1);
        printf("%i\n", numeroint);
        printf("%.2f\n", numerofloat);
        printf("%f\n", double01);
        
    return 0;
}
